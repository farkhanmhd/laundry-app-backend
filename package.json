{
  "name": "laundry-backend",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/server.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server src/server.ts",
    "lint": "bunx ultracite check",
    "start": "./server",
    "format": "bunx ultracite fix",
    "drizzle:generate": "bunx --bun drizzle-kit generate",
    "drizzle:migrate": "bunx --bun drizzle-kit migrate"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.4.1",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.6",
    "better-auth": "^1.3.34",
    "drizzle-orm": "^0.44.7",
    "drizzle-typebox": "^0.3.3",
    "elysia": "1.4.13",
    "elysia-remote-dts": "^1.0.3",
    "nanoid": "^5.1.6",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "bun-types": "^1.3.1",
    "drizzle-kit": "^0.31.6",
    "ultracite": "5.4.6"
  },
  "module": "src/index.js"
}
